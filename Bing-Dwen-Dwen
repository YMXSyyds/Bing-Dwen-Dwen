import turtle

__Pen = turtle.Pen()


# 速度
__Pen.speed(10)
# 左手
__Pen.penup()
__Pen.goto(176, 111)
__Pen.pencolor('lightgray')
__Pen.pensize(3)
__Pen.fillcolor('white')
__Pen.begin_fill()
__Pen.pendown()
__Pen.setheading(80)
__Pen.circle((-45), 210)
__Pen.circle((-290), 25)
__Pen.end_fill()
# 左手内
__Pen.penup()
