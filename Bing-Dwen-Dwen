# 五环
__Pen.penup()
__Pen.goto((-5), (-170))
__Pen.pendown()
__Pen.pencolor('blue')
__Pen.circle(6)
__Pen.penup()
__Pen.goto(10, (-170))
__Pen.pendown()
__Pen.pencolor('black')
__Pen.circle(6)
__Pen.penup()
__Pen.goto(25, (-170))
__Pen.pendown()
