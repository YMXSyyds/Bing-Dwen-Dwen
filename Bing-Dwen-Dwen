__Pen.penup()
__Pen.goto(2, (-175))
__Pen.pendown()
__Pen.pencolor('lightgoldenrod')
__Pen.circle(6)
__Pen.penup()
__Pen.goto(16, (-175))
__Pen.pendown()
__Pen.pencolor('green')
__Pen.circle(6)
__Pen.penup()

__Pen.pencolor('black')
__Pen.goto((-16), (-160))
__Pen.write("BEIJING 2022", font=('Arial', 10, 'bold italic'))
__Pen.hideturtle()

turtle.done()
